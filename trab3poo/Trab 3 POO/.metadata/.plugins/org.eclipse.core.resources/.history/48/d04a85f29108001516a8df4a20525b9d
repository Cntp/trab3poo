package trab3;

public class Livro {
	String Titulo;
	String Autor; 
	int Quantidade; //Qntd disponivel desse livro
	boolean Disponivel;
	int Cod; //Codigo para identificacao
	
	
	public Livro(int Pags, String Autor, int Quantidade, boolean Disponivel){ //Incompleto
		this.Pags = Pags;
		this.Autor = Autor;
		this.Quantidade = Quantidade;
		this.Disponivel = Disponivel;
		
		
		
	}
	
}
