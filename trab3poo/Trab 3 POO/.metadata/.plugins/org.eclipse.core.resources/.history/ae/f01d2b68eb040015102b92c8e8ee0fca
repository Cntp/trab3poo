	public void Cadastrar_Usuario();
	
	public void Reg_Devolucoes();
	public void Reg_Emprestimo();
	
	public void Avisar_Atraso();
	
	public int Calc_Atraso();
	public int Calc_Suspensao();
	
	public void Listar_Livros();
	public void Listar_Usuarios();
	public void Listar_Emprestimos();

}
