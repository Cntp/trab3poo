package trab3;

public class Usuario {
	
	String Nome;
	int numEmprestimos;
	boolean Atrasado;
	int ID; //Numero da pessoa dentro do Sistema
	int[] CodLivros; //Codigos dos livros emprestados
	
	int TMax;
	int NMax;
	
	public Usuario(int TMax,int NMax){
		
		this.TMax = TMax;
		this.NMax = NMax;
		
	}
	

}
